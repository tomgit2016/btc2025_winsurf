# Tennis Club Booking Configuration
TENNIS_CLUB_URL=https://www.burnabytennis.ca/
USERNAME=your_email@example.com
PASSWORD=your_password

# Booking preferences
PREFERRED_COURTS=3,4,5,2  # Comma-separated list, tried in order
PREFERRED_TIME=21:00  # Preferred booking start time (24h format)
BOOKING_DAYS_AHEAD=7  # How many days in advance to book
DURATION_MINUTES=120  # Desired booking duration in minutes (e.g., 120 for 2 hours)

# Additional players (if the site requires listing players on booking)
PLAYER1_NAME=Ian Zhu
PLAYER2_NAME=Will Yang
PLAYER3_NAME=Neil Chen

# AWS SNS Configuration for SMS Notifications
# Get these from AWS IAM Console: https://console.aws.amazon.com/iam/home#/security_credentials
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2  # Change to your preferred AWS region
# E.164 format: +1XXXXXXXXXX (country code + number)
SMS_PHONE_NUMBER=+16045551234

# Optional: Set to 'false' to disable SMS notifications
ENABLE_SMS_NOTIFICATIONS=true
